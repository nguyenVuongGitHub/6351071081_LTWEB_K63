@model IEnumerable<QLBanSach.Models.Entities.NHAXUATBAN>

<ul class="left_menu">
    @foreach (var item in Model.Select((value, index) => new { value, index }))
    {
        <li class="@(item.index % 2 == 0 ? "even" : "odd")">
            <a href="@Url.Action("SPTheoNXB", "BookStore", new { MaNXB = item.value.MaNXB })">@item.value.TenNXB</a>
        </li>
    }
</ul>